      00002F                         77 select_uart:
      00002F 52 02            [ 2]   78     sub sp,#LOCAL_SIZE
      000031 48               [ 1]   79     sll a 
      000032 6B 02            [ 1]   80     ld (ACC16+1,sp),a 
      000034 0F 01            [ 1]   81     clr (ACC16,sp) 
      000036 AE 00 05         [ 2]   82     ldw x,#uart_regs 
      000039 72 FB 01         [ 2]   83     addw x,(ACC16,sp)
      00003C FE               [ 2]   84     ldw x,(x)
      00003D 5B 02            [ 2]   85     addw sp,#LOCAL_SIZE 
      00003F 81               [ 4]   86     ret
