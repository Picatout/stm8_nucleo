                                     88 ;----------------------------------------------
                                     89 ; initialize UART peripheral
                                     90 ; initilialize config: 8N1 no flow control
                                     91 ; C prototype:  void uart_init(uint8_t baud,uint8_t uart_id)
                                     92 ; input:
                                     93 ;   baud        uint8_t baud
                                     94 ;   uart_id     uin8t_t uart selector {UART1,UART3}
                                     95 ; output:
                                     96 ;   none
                                     97 ;---------------------------------------------
                           000004    98     ARG_OFS=4  
                           000005    99     BAUD=ARG_OFS+1 
                           000006   100     UART=ARG_OFS+2
                                    101 ; local var 
                           000001   102     ACC16=1
                           000002   103     LOCAL_SIZE=2     
      000040                        104 _uart_init::
      000040                        105 uart_init::
      000040 52 02            [ 2]  106     sub sp,#LOCAL_SIZE 
      000042 0F 01            [ 1]  107     clr (ACC16,sp)
                                    108 ;configure port
      000044 7B 06            [ 1]  109     ld a,(UART,sp)
      000046 26 0E            [ 1]  110     jrne uart3
      000048                        111 uart1:
      000048 72 1A 50 02      [ 1]  112 	bset PA_DDR,#UART1_TX_PIN ; tx pin
      00004C 72 1A 50 03      [ 1]  113 	bset PA_CR1,#UART1_TX_PIN ; push-pull output
      000050 72 1A 50 04      [ 1]  114 	bset PA_CR2,#UART1_TX_PIN ; fast output
      000054 20 0C            [ 2]  115     jra config_baud 
      000056                        116 uart3:
      000056 72 1A 50 11      [ 1]  117 	bset PD_DDR,#UART3_TX_PIN ; tx pin
      00005A 72 1A 50 12      [ 1]  118 	bset PD_CR1,#UART3_TX_PIN ; push-pull output
      00005E 72 1A 50 13      [ 1]  119 	bset PD_CR2,#UART3_TX_PIN ; fast output
      000062                        120 config_baud:
      000062 CD 00 2F         [ 4]  121     call select_uart 
                                    122 ; check whick clock is active    
      000065 A6 B4            [ 1]  123     ld a,#CLK_SWR_HSE ; HSE clock 8Mhz 
      000067 C1 50 C3         [ 1]  124     cp a,CLK_CMSR
      00006A 26 06            [ 1]  125     jrne 4$ 
      00006C 90 AE 00 09      [ 2]  126     ldw y,#baud_fcy8
      000070 20 04            [ 2]  127     jra 5$
      000072 90 AE 00 1B      [ 2]  128 4$: ldw y,#baud_fcy16 
      000076 7B 05            [ 1]  129 5$: ld a,(BAUD,sp)
      000078 48               [ 1]  130     sll a 
      000079 6B 02            [ 1]  131     ld (ACC16+1,sp),a 
