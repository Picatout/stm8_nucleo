      00003B 7B 02            [ 1]  118     ld a,(ACC24+1,sp)
      00003D 97               [ 1]  119     ld xl,a  ; produit final bits 15:8
      00003E 7B 03            [ 1]  120     ld a,(ACC24+2,sp) ; produit final bits 7:0
      000040 5B 03            [ 2]  121     addw sp,#LOCAL_SIZE 
